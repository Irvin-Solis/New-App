{
  "name": "Auto-Reminder",
  "version": "1.0.0",
  "description": "Auto creates a comment with a deadline on a pull request.",
  "author": "Irvin Jimenez-Solis &lt;ijimenez.solis18@gmail.com&gt; (https://github.com/Irvin-Solis)",
  "license": "ISC",
  "repository": "https://github.com/Irvin-Solis/github-app.git",
  "homepage": "https://github.com/Irvin-Solis/github-app",
  "bugs": "https://github.com/Irvin-Solis/github-app/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon --exec \"npm start\"",
    "start": "probot run ./index.js",
    "now-start": "PRIVATE_KEY=$(echo $PRIVATE_KEY_BASE64 | base64 -d) npm start",
    "lint": "standard --fix",
    "test": "jest --coverage",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "probot": "^7.2.0"
  },
  "devDependencies": {
    "jest": "^22.4.3",
    "nodemon": "^1.17.2",
    "prettier": "1.13.7",
    "smee-client": "^1.0.2",
    "standard": "^10.0.3"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  }
}
